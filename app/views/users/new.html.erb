<h1>New User!</h1>

<form action="<%=users_path %>" method="post">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

  <label for="user_email">Email Address:</label>
  <input
    type="text"
    name="user[email]"
    value="<%=@user.email %>">

  <br>

  <label for="user_password">Password:</label>
  <input
    type="password"
    name="user[password]"
    value="<%=@user.password%>">

  <input type="submit">
</form>
